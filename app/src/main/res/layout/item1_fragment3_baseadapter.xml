<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="200dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="20dp"
    app:cardElevation="5dp"
    app:cardBackgroundColor="#88f"
    app:cardUseCompatPadding="true"
    app:contentPadding="5dp"
    android:id="@+id/cardview_fragmet3_baseadapterId"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:id="@+id/image11_fragment3_baseadapterId"
            android:scaleType="fitXY"
            android:adjustViewBounds="true"
            android:src="@drawable/shishi"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <ImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/image12_fragment3_baseadapterId"
                android:src="@drawable/shishi"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:layout_margin="5dp"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="5dp"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:id="@+id/image13_fragment3_baseadapterId"
                android:src="@drawable/shishi"
                />

        </LinearLayout>


    </LinearLayout>



</android.support.v7.widget.CardView>
<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:id="@+id/cardview_fragmet3_baseadapterId">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
           <RelativeLayout
               android:layout_width="0dp"
               android:layout_height="match_parent"
               android:layout_weight="1"
               >
               <ImageView
                   android:layout_width="100dp"
                   android:layout_height="100dp"
                   android:scaleType="fitXY"
                   android:adjustViewBounds="true"
                   android:id="@+id/image21_fragment3_baseadapter3Id"
                   />
               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="蒙面唱歌"
                   android:layout_marginLeft="5dp"
                   android:textSize="10sp"
                   android:layout_marginTop="60dp"
                   android:id="@+id/textView21_fragment3_baseadapterId"
                   />
               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="蒙面唱歌"
                   android:textSize="8sp"
                   android:layout_marginLeft="5dp"
                   android:layout_marginTop="80dp"
                   android:id="@+id/textView22_fragment3_baseadapterId"
                   />
           </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/shishi"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:id="@+id/image22_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="5dp"
                    android:textSize="10sp"
                    android:layout_marginTop="60dp"
                    android:id="@+id/textView23_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="80dp"
                    android:id="@+id/textView24_fragment3_baseadapterId"
                    />
           </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:id="@+id/image23_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="5dp"
                    android:textSize="10sp"
                    android:layout_marginTop="60dp"
                    android:id="@+id/textView25_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="80dp"
                    android:id="@+id/textView26_fragment3_baseadapterId"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:src="@drawable/shishi"
                    android:id="@+id/image24_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="5dp"
                    android:textSize="10sp"
                    android:layout_marginTop="60dp"
                    android:id="@+id/textView27_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="80dp"
                    android:id="@+id/textView28_fragment3_baseadapterId"
                    />
            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>

</android.support.v7.widget.CardView>
<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="200dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="20dp"
    app:cardElevation="5dp"
    app:cardBackgroundColor="#88f"
    app:cardUseCompatPadding="true"
    app:contentPadding="5dp"
    android:id="@+id/cardview_fragmet3_baseadapterId"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:id="@+id/image41_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginTop="50dp"
                    android:textSize="10sp"
                    android:layout_marginLeft="10dp"
                    android:id="@+id/textView41_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="70dp"
                    android:id="@+id/textView42_fragment3_baseadapterId"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:src="@drawable/shishi"
                    android:id="@+id/image42_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="10sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="50dp"
                    android:id="@+id/textView43_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="70dp"
                    android:id="@+id/textView44_fragment3_baseadapterId"
                    />
            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:src="@drawable/shishi"
                    android:id="@+id/image43_fragment3_baseadapter3Id"
                    android:layout_alignParentBottom="true" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="10dp"
                    android:textSize="10sp"
                    android:layout_marginTop="30dp"
                    android:id="@+id/textView45_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="8sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="45dp"
                    android:id="@+id/textView46_fragment3_baseadapterId"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:src="@drawable/shishi"
                    android:id="@+id/image44_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="10sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="30dp"
                    android:id="@+id/textView47_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="10dp"

                    android:textSize="8sp"
                    android:layout_marginTop="45dp"
                    android:id="@+id/textView48_fragment3_baseadapterId"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/shishi"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:id="@+id/image45_fragment3_baseadapter3Id"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:textSize="10sp"
                    android:layout_marginLeft="10dp"

                    android:layout_marginTop="30dp"
                    android:id="@+id/textView49_fragment3_baseadapterId"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="蒙面唱歌"
                    android:layout_marginLeft="5dp"
                    android:textSize="8sp"
                    android:layout_marginTop="45dp"
                    android:id="@+id/textView410_fragment3_baseadapterId"
                    />
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>


</android.support.v7.widget.CardView>